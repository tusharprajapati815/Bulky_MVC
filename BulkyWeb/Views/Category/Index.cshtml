@model List<Category>

<h1>Category List</h1>

<table class="table table-bordered table-striped">
	<thead>
		<tr>
			<th>ID</th>
			<th>Category Name</th>
			<th>DisplayOrder</th>
		</tr>
	</thead>
	<tbody>
		@foreach(var item in Model)
		{
			<tr>
				<td>@item.Id</td>
				<td>@item.Name</td>
				<td>@item.DisplayOrder</td>
			</tr>
		}
	</tbody>
</table>